{
    "name": "redcode/aerospike-cache-provider",
    "description": "Adds aerospike provider to use it through the doctrine/cache",
    "keywords": ["aerospike", "doctrine", "cache", "provider"],
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Reddikh",
            "email": "andrew@reddikh.com"
        },
        {
            "name": "RedCode",
            "homepage": "http://redcode.im"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "RedCode\\Common\\Cache": "src/" }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": "^5.4",
        "doctrine/cache": "^1.4"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.10",
        "contorion/aerospike-stub": "^1.0",
        "phpunit/phpunit": "^4.8"
    }
}
